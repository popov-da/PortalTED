<?xml version="1.0" encoding="utf-8"?><!--Created by VME v.14.2.0.0 (build 14.2.0.16)--><SimulationInteractivity xmlns="http://services.parallelgraphics.com/vm/mmr/vm-interactivity-xml/all"><SimulationInformation><ExporterVersionNumber>14.2.0.16</ExporterVersionNumber><ModificationDate>11/21/24</ModificationDate><SpecID>S1000D4_1_TEXT</SpecID><SpecVersion>1.14</SpecVersion><SpecLang>ru</SpecLang><XMLFileName>MTPM-A-D2-90-01-00A-041A-A</XMLFileName><DocumentFile>DMC-MTPM-A-D2-90-01-00A-041A-A_001-01_ru-RU.xml</DocumentFile><Options><Value name="3DFramePosition" type="string">Right</Value><Value name="3DFrameSize" type="string">40</Value><Value name="SelectedHotspotColor" type="color">#3152BA</Value><Value name="PDFPublish" type="boolean">No</Value><Value name="PublicPath" type="string">../res/</Value><Value name="UseSVGFilesOnly" type="boolean">No</Value><Value name="SingleHTMLBundle" type="boolean">No</Value><Value name="UseViewerBundle" type="string">0</Value><Value name="UseCompressedSVG" type="boolean">No</Value><Value name="DisableHTMLCreation" type="boolean">No</Value></Options><metadata><value name="infoCodeVariant" decl-id="ICV" type="string">A</value><value name="itemLocationCode" decl-id="ILC" type="string">A</value><value name="issueType" decl-id="ISSUE_TYPE" type="string">new</value><value name="issueNumber" decl-id="ISSUE_NO" type="integer">1</value><value name="inWork" decl-id="ISSUE_REVNO" type="integer">1</value><value name="Schema" decl-id="SCHEMA" type="string">descript</value><value name="$S1000D Issue" decl-id="S1000D_ISSUE" type="string">S1000D_4-1</value><value name="applicDescription" decl-id="APPLIC_DESCR" type="string">ВСЕ</value><value name="originator" decl-id="ORIGINATOR" type="string">МОДУЛЬ МКД</value><value name="originatorCode" decl-id="ORIGINATORCODE" type="string">MDMKD</value><value name="responsiblePartnerCompany" decl-id="RPC" type="string">МОДУЛЬ МКД</value><value name="responsiblePartnerCompanyCode" decl-id="APPLIC_MFC" type="st